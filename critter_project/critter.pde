abstract class Critter{
  int health, age, luck;
  
  void setAge(int _age){
    age = _age;
  }
  int getAge(){
    return age;
  }
  void setHealth(int _health){
    health = _health;
  }
  int getHealth(){
    return health;
  }
  void setLuck(int _luck){
    luck = _luck;
  }  
  int getLuck(){
    return luck;
  }
}
